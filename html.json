        "description": "container"
    },
    "ngClass": {
        "prefix": "ngClass",
        "body": "[ngClass]=\"${1:expression} ? '${2:cssClass1}' : '${3:cssClass2}'\"",
        "description": "Angular ngClass"
    },
    "class": {
        "prefix": [
            "cla",
            "class",
            "cl",
            "clas"
        ],
        "body":"class=\"$0\"",
        "description": "make a class"
    },
    "ngfor": {
        "prefix": "ngFor",
        "body": "*ngFor=\"let ${1:item} of ${2:list}\"${0}",
        "description": "Angular *ngFor"
    },
    "div": {
        "prefix": "div",
        "body": "<div class=\"$1\">$2",
        "description": "div"
    },
    "onClick": {
        "prefix": [
            "(cli",
            "click",
            "(clic"
        ],
        "body": "(click)=\"$0\"",
        "description": "add onClick"
    }
